{
  "name": "test-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "USE_PROXY_MODE=true playwright test --config=tests/playwright.config.mjs",
    "test:e2e:log": "USE_PROXY_MODE=true playwright test --config=tests/playwright.config.mjs 2>&1 | tee e2e-run.log",
    "test:e2e:ui": "playwright test tests/e2e/ --ui",
    "test:e2e:debug": "playwright test tests/e2e/ --debug",
    "test:e2e:headed": "playwright test tests/e2e/ --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:monitor": "../scripts/monitor-e2e-tests.sh",
    "test:dev": "vite & playwright test tests/e2e/ --headed",
    "test:e2e:openai": "USE_PROXY_MODE=true playwright test tests/e2e/openai-proxy-e2e.spec.js tests/e2e/openai-proxy-tts-diagnostic.spec.js tests/e2e/greeting-playback-validation.spec.js tests/e2e/openai-inject-connection-stability.spec.js tests/e2e/callback-test.spec.js --config=tests/playwright.config.mjs",
    "test:e2e:6b": "USE_PROXY_MODE=true playwright test tests/e2e/openai-proxy-e2e.spec.js --grep \"6b.*462\" --config=tests/playwright.config.mjs",
    "test:e2e:deepgram": "E2E_BACKEND=deepgram USE_PROXY_MODE=true playwright test --config=tests/playwright.config.mjs",
    "backend": "node scripts/backend-server.js",
    "openai-proxy:cli": "npm run openai-proxy:cli --prefix .. --",
    "test:e2e:proxy": "playwright test tests/e2e/deepgram-backend-proxy-mode.spec.js --config=tests/playwright.config.mjs",
    "test:e2e:proxy:log": "USE_PROXY_MODE=true playwright test --config=tests/playwright.config.mjs 2>&1 | tee e2e-proxy-run.log"
  },
  "dependencies": {
    "@signal-meaning/voice-agent-react": "file:../",
    "dotenv": "^17.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jest": "^30.2.0",
    "selfsigned": "^2.4.1",
    "ts-jest": "^29.4.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
